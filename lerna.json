{
  "packages": [
    "example",
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master"
      ],
      "distTag": "latest",
      "ignoreChanges": [
        "*.md",
        "**/*.test.ts",
        "**/*.test.js"
      ],
      "message": "chore(release): publish %s"
    },
    "version": {
      "message": "chore(release): %s"
    }
  },
  "version": "0.1.0"
}
